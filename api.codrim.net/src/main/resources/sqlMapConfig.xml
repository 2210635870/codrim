<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "mybatis-3-config.dtd">

<configuration>
    <!-- - - - - - - 懒加载和缓存 - - - - - - - - - - -->
    <settings>
        <setting name="cacheEnabled" value="false"/>
        <setting name="lazyLoadingEnabled" value="true"/>
        <setting name="aggressiveLazyLoading" value="false"/>
    </settings>
    <!-- - - - - - - -映射文件路径- - - - - - -->
    <mappers>
        <mapper resource="common/codrim/sql/TbProductMapper.xml"/>
         <mapper resource="common/codrim/sql/TbProductDetailMapper.xml"/>
                   <mapper resource="common/codrim/sql/TbRandomSendNumMapper.xml"/>
                           <mapper resource="common/codrim/sql/TbEffectRankingsMapper.xml"/>
                           <mapper resource="common/codrim/sql/TbChannelMapper.xml"/>
                           
    </mappers>
</configuration>
